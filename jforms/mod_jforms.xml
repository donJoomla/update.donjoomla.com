---
title: JForms Module
description: JForms Module
element: mod_jforms
type: module
infourl: http://donjoomla.com/extensions/jforms/
---
<updates>
  {% for post in site.pages %}
  {% if post.category == mod_jforms %}
  <update>
    <name>{{page.title}}</name>
    <description>{{page.description}}</description>
    <element>{{page.element}}</element>
    <type>{{page.type}}</type>
    <client>0</client>
    <client_id>0</client_id>
    <version>{{post.title}}</version>
    <infourl title="{{page.title}}">{{page.infourl}}</infourl>
    <downloads>
        <downloadurl type="full" format="zip">{{post.download}}</downloadurl>
    </downloads>
    <maintainer>Adam Bouqdib</maintainer>
    <maintainerurl>http://donjoomla.com</maintainerurl>
    <targetplatform name="joomla" version="{{post.platform}}"/>
  </update>
  {% endif %}
  {% endfor %}
</updates>
