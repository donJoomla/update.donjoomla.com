---
title: JForms YMLP Plugin
description: JForms Your Mailing List Provider Plugin
element: ymlp
type: plugin
folder: jforms
infourl: http://donjoomla.com/extensions/jforms/your-mailing-list-provider
---
<updates>
  {% for post in site.categories.mod_jforms %}
  <update>
    <name>{{page.title}}</name>
    <description>{{page.description}}</description>
    <element>{{page.element}}</element>
    <type>{{page.type}}</type>
    <folder>{{page.folder}}</folder>
    <client>0</client>
    <version>{{post.version}}</version>
    <infourl title="{{page.title}}">{{page.infourl}}</infourl>
    <downloads>
        <downloadurl type="full" format="zip">{{post.download}}</downloadurl>
    </downloads>
    <maintainer>Adam Bouqdib</maintainer>
    <maintainerurl>http://donjoomla.com</maintainerurl>
    <targetplatform name="joomla" version="{{post.platform}}"/>
  </update>
  {% endfor %}
</updates>
