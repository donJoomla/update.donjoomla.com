---
title: JForms Google Spreadsheets Plugin
description: Google Spreadsheet Export
element: googlespreadsheet
type: plugin
infourl: http://donjoomla.com/extensions/jforms/google-spreadsheets
platform: 2.5
---
{% for post in site.categories.plg_jforms_googlespreadsheet limit:1 %}
<update>
	<name>{{page.title}}</name>
    <description>{{page.description}}</description>
    <element>{{page.element}}</element>
    <type>{{page.type}}</type>
    <version>{{post.version}}</version>
    <infourl title="{{page.title}}">{{page.infourl}}</infourl>
    <downloads>
        <downloadurl type="full" format="zip">{{post.download}}</downloadurl>
    </downloads>
    <tags>
        <tag>stable</tag>
    </tags>
    <maintainer>Adam Bouqdib</maintainer>
    <maintainerurl>http://donjoomla.com</maintainerurl>
    <targetplatform name="joomla" version="{{page.platform}}"/>
</update>
{% endfor %}